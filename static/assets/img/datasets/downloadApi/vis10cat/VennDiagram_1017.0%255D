<script>
$(window).load(function(){
   //-- Added by Chris Case 00247769
   function addSchools(schoolName, schoolLink, schoolBefore){
        var s = schoolName.length;
        for(var i = 0; i < s; i++ ){
            if(schoolBefore[i] != ''){
                $('<li><a href="'+ schoolLink[i] +'" tabindex="-1">'+ schoolName[i] +'</a></li>').insertBefore($('.schoollist .sw-dropdown-list li:contains("'+ schoolBefore[i] +'")'));
            } else {
                $('<li><a href="'+ schoolLink[i] +'" tabindex="-1">'+ schoolName[i] +'</a></li>').appendTo($('.schoollist .sw-dropdown-list'));
            }
        }
    }
    addSchools(
    	/*Lyndsay added Chief Joseph Elementary School and Alliance High School at Meek - case 00350998 */
        /*Lyndsay commented out all schools below except Virtual Scholars - Case 00385631 */
       /* School Names */ [/*'Odyssey Program', 'ACCESS Academy Alternative Program', 'Pioneer Special School Program', */'Virtual Scholars'/*, 'Chief Joseph Elementary School', 'Alliance High School at Meek'*/], 
       /* School Links */ [/*'http://www.pps.k12.or.us/schools/odyssey/', 'http://www.pps.k12.or.us/schools/access/',*/'//www.pps.k12.or.us/schools/pioneer/'/*, 'http://vs.pps.net/site/default.aspx?PageID=1', 'http://www.pps.k12.or.us/schools/cjog/', 'http://www.pps.k12.or.us/schools/alliance/meek/'*/], 
       /*Schools to be Inserted Before */ [/*'Peninsula', 'Ainsworth', 'Portland Even', */'West Sylvan'/*, 'Clarendon Regional Early Learning Academy', 'Arleta K-8'*/]
    );//-- End addSchools
  //-- End Case 00247769
    //-- Commented out by Chris To use code above
   /*$("<li><a href='http://www.pps.k12.or.us/schools/access/'>ACCESS Academy Alternative Program</a></li>").appendTo(".sw-mystart-dropdown.schoollist .sw-dropdown-list");
   $("<li><a href='http://www.pps.k12.or.us/schools/odyssey/'>Odyssey Program</a></li>").appendTo(".sw-mystart-dropdown.schoollist .sw-dropdown-list");
   $("<li><a href='http://www.pps.k12.or.us/schools/pioneer/'>Pioneer Special School Program</a></li>").appendTo(".sw-mystart-dropdown.schoollist .sw-dropdown-list");
   $("<li><a href='http://vs.pps.net/site/default.aspx?PageID=1'>Virtual Scholars</a></li>").appendTo(".sw-mystart-dropdown.schoollist .sw-dropdown-list");*/
});//End Load
</script>

<script type='text/javascript'>
    //09.29.2016 - Added by Jacinda 
    //Sets search to custom google allows for results from subsites 
    $(function(){
		var searchID = "018342302013277717360:zcdr8tuajjq";
		$("#sw-search-button").click(function(e){
		    e.preventDefault();
		    var searchForm = $(this).parent();
		    window.location.href = "//www.google.com/cse?cx="+searchID+"&hq=more:pagemap:document-siteid:4&ie=UTF-8&q="+$("#sw-search-input", searchForm).val()+"#gsc.tab=0&gsc.q="+$("#sw-search-input", searchForm).val()+"&gsc.sort=";
            e.stopImmediatePropagation();
		});
        $("#sw-search-input").keyup(function(e) {
          if (e.which == 13) {
          	e.preventDefault();
		    var searchForm = $(this).parent();
		    window.location.href = "//www.google.com/cse?cx="+searchID+"&hq=more:pagemap:document-siteid:4&ie=UTF-8&q="+$("#sw-search-input", searchForm).val()+"#gsc.tab=0&gsc.q="+$("#sw-search-input", searchForm).val()+"&gsc.sort=";
            e.stopImmediatePropagation();
          }
        });
		$("#sw-search-input").focus(function() {
		    if($(this).val() == "Search") {
		        $(this).val("");
		    }
		});
		$("#sw-search-input").blur(function() {
		    if($(this).val() == "") {
		        $(this).val("Search");
		    }
		});
	});
</script>
<script type='text/javascript'>
    //09.29.2016 - Added by Jacinda 
    //Sets search to custom google allows for results from subsites 
    $(window).load(function() {
    	if($(window).width() < 1024) {
          var searchID = "018342302013277717360:zcdr8tuajjq";
          $("#rs-menu-search-button").click(function(e){
              e.preventDefault();
              var searchForm = $(this).parent();
              window.location.href = "//www.google.com/cse?cx="+searchID+"&hq=more:pagemap:document-siteid:4&ie=UTF-8&q="+$("#rs-menu-search-input", searchForm).val()+"#gsc.tab=0&gsc.q="+$("#rs-menu-search-input", searchForm).val()+"&gsc.sort=";
              e.stopImmediatePropagation();
          });
          $("#rs-menu-search-input").keyup(function(e) {
            if (e.which == 13) {
              e.preventDefault();
              var searchForm = $(this).parent();
              window.location.href = "//www.google.com/cse?cx="+searchID+"&hq=more:pagemap:document-siteid:4&ie=UTF-8&q="+$("#rs-menu-search-input", searchForm).val()+"#gsc.tab=0&gsc.q="+$("#rs-menu-search-input", searchForm).val()+"&gsc.sort=";
              e.stopImmediatePropagation();
            }
          });
          $("#rs-menu-search-input").focus(function() {
              if($(this).val() == "Search") {
                  $(this).val("");
              }
          });
          $("#rs-menu-search-input").blur(function() {
              if($(this).val() == "") {
                  $(this).val("Search");
              }
          });
        }
	});
</script>
