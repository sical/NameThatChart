				scrollLock2 = 0;
			}
		} else {
			if(scrolled >= position && scrollLock2 == 0) {
				$("#gb-channel-list-outer").addClass("fixed-header");
				$("#sp-content").css("padding", $("#gb-channel-list-outer").height()+"px 0 0 0");
				scrollLock2 = 1;
			} else if(scrolled < position && scrollLock2 == 1){
				$("#gb-channel-list-outer").removeClass("fixed-header");
				$("#sp-content").css("padding", "0");
				scrollLock2 = 0;
			}		
		}*/
	}
	
	function headlineHeight() {
		//GET HEIGHT OF EACH ARTICLE IN CONT. 4 ON HP
        	var maxHeight = -1;
		$('#content-left-top #sw-content-container4 .ui-articles li').not($(".ui-widget.app.portland-headlines .ui-articles li")).each(function() {
			maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
		});
		//SET LARGEST HEIGHT ON EACH ARTICLE IN CONT. 4 ON HP
		$('#content-left-top #sw-content-container4 .ui-articles li').not($(".ui-widget.app.portland-headlines .ui-articles li")).each(function() {
			$(this).height(maxHeight);
		});
	}
	
	function languageDropdown() {
		var csLanguages = "<a href='/Page/1085'>中文</a>" +
				  "<a href='/Page/1086'>Русский</a>" +
				  "<a href='/Page/1087'>Soomaali</a>" +//Change from Soomaaliga to Soomaali By Chris Case 00182385
				  "<a href='https://www.pps.net/Page/1088'>Español</a>" +
				  "<a href='https://www.pps.net/Page/1089'>Tiếng Việt</a>";
		
		$("<div id='cs-language-outer' class='hide768'><div id='cs-language-inner'>" + csLanguages + "</div></div>").appendTo("#sw-mystart-left");
		
		//768 DROPDOWN	
		var csLanguages768 = "<li><a href='/Page/1085'>中文</a></li>" +
				  "<li><a href='/Page/1086'>Русский</a></li>" +
				  "<li><a href='/Page/1087'>Soomaaliga</a></li>" +
				  "<li><a href='https://www.pps.net/Page/1088'>Español</a></li>" +
				  "<li><a href='https://www.pps.net/Page/1089'>Tiếng Việt</a></li>";	  
				  
		$("<div id='cs-language-cont' class='show768'><div class='cs-language-btn'><span>Languages</span></div><ul class='cs-language-dropdown'>" + csLanguages768 + "</ul></div>").appendTo("#sw-mystart-left");
		
		$(".cs-language-btn").click(function() {
			$(".cs-language-dropdown").slideToggle();
		});
		
		$(".cs-language-dropdown li").each(function() {
			var checkImage = $("a img",this).attr("src");
			if(checkImage == "" || checkImage == "/cms/lib/SW00000016/Centricity/Template/GlobalAssets/images///Faces/default-man.jpg") {
				$(this).parent().parent().hide();
			}
		});
	}
	
	$(function(){
		// SET IE CLASSES TO BODY
		if(isMSIE){ $("body").addClass("msie"); }
		if(isMSIE7){ $("body").addClass("msie7"); }
		if(isMSIE8){ $("body").addClass("msie8"); }
		if(isMSIE9){ $("body").addClass("msie9"); }
		if(isMSIE10){ $("body").addClass("msie10"); }
		$("body").addClass(navigator.platform + " " + browserName + " " + browserName + browserVersion);
		
		
			$("#sw-mystart-outer").addClass("cs-notlogged");
		
	
		$(window).bind("resize", function(){ 
			pgScreenSize(); 
			imgResize();
		});
		
		var socialIconsMenu = '1,Facebook,https://www.facebook.com/pps.homepage/,_blank;' +
				      '1,Twitter,https://twitter.com/PPSConnect,_blank;' +
				      '1,YouTube,https://www.youtube.com/user/ppscomms,_blank;' +
				      '1,Instagram,#,_blank;' +
				      '1,Flickr,https://www.flickr.com/photos/41430185@N08/albums,_blank;' +
				      '1,Pinterest,#,_blank;' +
				      '1,LinkedIn,https://www.linkedin.com/company/portland-public-schools,_blank;' +
				      '1,Google +,#,_blank;';
		
		// RESPONSIVE MENU
		var csMenuLanguages = "2,中文,/Page/1085,_self;" +
				  "2,Русский,/Page/1086,_self;" +
				  "2,Soomaaliga,/Page/1087,_self;" +
				  "2,Español,https://www.pps.net/Page/1088,_self;" +
				  "2,Tiếng Việt,https://www.pps.net/Page/1089,_self;";
				  
		$("body").csRsMenu({
			"breakPoint"		: 640, // SYSTEM BREAK POINTS - 768, 640, 480, 320
			"primaryColor"		: "#1A1A1A", // DEFAULT - #1A1A1A
		        "secondaryColor"        : "#0C0C0C", // DEFAULT - #0C0C0C
		        "accentColor"           : "#BABABA", // DEFAULT - #BABABA
			"menuButtonParent"	: "#sw-mystart-inner",
			"showMenuBtnText"	: "yes",
			"menuBtnText"		: "Top Menu",
			"showSchools"		: "no",
			"showTranslate"		: "no",
			"translateId"		: "none",
			"showAccount"		: "yes",
			"extraMenuOptions"	: "Social Media; Languages", // EXTRA MENU HEADING NAMES SEPARATED BY A ;
			"extraMenuItems"	: socialIconsMenu + csMenuLanguages, // EXTRA MENU BUTTONS - STRUCTURE IS: MENU-OPTION-NUMBER, ITEM-NAME, ITEM-URL, ITEM-TARGET;
			"siteID"		: "4"
		});
		$(".rs-menu-option.social-media .rs-menu-option-sublist li:eq(2)").addClass('true');
		$(".rs-menu-option.social-media .rs-menu-option-sublist li:eq(3)").addClass('true');
		$(".rs-menu-option.social-media .rs-menu-option-sublist li:eq(4)").addClass('true');
		$(".rs-menu-option.social-media .rs-menu-option-sublist li:eq(5)").addClass('false');
		$(".rs-menu-option.social-media .rs-menu-option-sublist li:eq(6)").addClass('true');
		$(".rs-menu-option.social-media .rs-menu-option-sublist li:eq(7)").addClass('false');
		$(".rs-menu-option.social-media .rs-menu-option-sublist li:eq(8)").addClass('true');
		$(".rs-menu-option.social-media .rs-menu-option-sublist li:eq(9)").addClass('false');
		
		// RESPONSIVE PAGELIST MENU
		$("body").csRsPagelistMenu({
			"breakPoint"		: 640, // SYSTEM BREAK POINTS - 768, 640, 480, 320
			"primaryColor"		: "#1A1A1A", // DEFAULT - #1A1A1A
		        "secondaryColor"        : "#0C0C0C", // DEFAULT - #0C0C0C
		        "accentColor"           : "#BABABA", // DEFAULT - #BABABA
			"menuButtonParent"	: "#sp-content",
			"showMenuBtnText"	: "yes",
			"menuBtnText"		: "PAGES",
			"siteID"		: "4"
		});
		
		$("#gb-channel-list").creativeNavMenu({
			"breakPoint"		: 1023, // SYSTEM BREAK POINTS - 1023, 767, 639, 479
			"menuPosition"		: "absolute", // OTHER OPTION IS 'relative'
			"menuButtonParent"	: "#gb-channel-list",
			"showMenuBtnText"	: "yes",
			"menuBtnText"		: "Main Menu",
			"onLoad"                : function(){ }
		});
		
		pgScreenSize();
		useChannelForSection();
		dynamicContent();
		
		if(!$("#content-left-top #sw-content-container3.ui-hp div.ui-widget.app.cs-rs-multimedia-rotator").length) {
			$("#content-left-top #sw-content-container3.ui-hp").hide();
		}	
		if(!$("#content-left-top .ui-widget.app").length) {
			$("#content-left-top").hide();
		}
		if(!$("#content-left-bottom .ui-widget.app").length) {
			$("#content-left-bottom").hide();
		}
		if(!$("#content-right-top .ui-widget.app").length) {
			$("#content-right-top").hide();
		}
		if(!$("#content-right-middle .ui-widget.app").length) {
			$("#content-right-middle").hide();
		}
		if(!$("#content-right-bottom .ui-widget.app").length) {
			$("#content-right-bottom").hide();
		}
	});	
	
	function pgScreenSize(){
		var getWidth = $(window).width();
		if (getWidth >= 1024) { // 960+
			mediaRotator();
			contentRotator();
			var logoWidth = $("#gb-logo img").width();
			var headerWidth = $("#gb-header").width();
			$("#gb-sitename").width();
			appViewReset();
		} if (getWidth >= 768 && getWidth < 1024) { // 768+
			mediaRotator();
			contentRotator();
			appViewReset();
		} if (getWidth >= 640 && getWidth < 768) { // 640+
			mediaRotator();
			contentRotator();
			appView();
		} if (getWidth >= 480 && getWidth < 640) { // 480+
			mediaRotator();
			contentRotator();
			appView();
		} if (getWidth >= 320 && getWidth < 480) { // 320+
			mediaRotator();
            appView();
			contentRotator();
		}
	}
	
	function mediaRotator(){
		if(!$("#sw-content-container10.region.ui-hp .rs-gallery-images").length){
			$("#sw-content-container10.ui-hp div.ui-widget.app.cs-rs-multimedia-rotator").csRsMultimediaRotator({
				"imageWidth" 			: 1500,
		        	"imageHeight" 			: 915,
		        	"appHeader" 			: "no",
		        	"titles" 			: "yes",
		        	"captions" 			: "yes",
		        	"galleryOverlay" 		: "no",
		       		"linkedElement" 		: ["image"], // ["image", "title", "overlay", "all"]
		        	"playPause" 			: "no",
		        	"mainControls" 			: "yes",
		        	"contMoreLink" 			: "no",
		        	"contMoreLinkText" 		: "MORE",
		        	"descMoreLink" 			: "yes",
		        	"descMoreLinkText" 		: "MORE",
		        	"bullets" 			: "no",
		        	"bulletControls" 		: "no",
		        	"thumbnails" 			: "no",
		        	"thumbnailWidth" 		: 122,
		        	"thumbnailHeight" 		: 53,
		        	"thumbnailSlideDirection" 	: "horizontal", // horizontal or vertical
		        	"hoverPause" 			: "yes",
		        	"fullScreenRotator" 		: "yes", // image dimensions work well at 1500x915
		        	"fullScreenBreakpoints"		: ["960"],
		        	"fullScreenVideo" 		: "yes",
		        	"transitionType" 		: "fade", // fade or slide
		        	"transitionSpeed" 		: 1,
		        	"transitionDelay" 		: 8,
				"autoRotate" 			: "yes",
		        	"onImageLoad" 			: function() { },
		        	"allImagesLoaded" 		: function() { },
		        	"onThumbnailLoad" 		: function() { },
		        	"allThumbnailsLoaded" 		: function() { },
		        	"onTransitionStart" 		: function() {
		        		$("#scroll-down-btn").addClass("bounce animated");
		        		if($("#hp-slideshow .rs-gallery-caption p").length) {
		        			if($("#hp-slideshow .rs-caption").hasClass("hidden active")) {
			        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-title");
			        		} else {
			        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-caption");
			        		}
		        		} else {
		        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-title");
		        		}
		        	 },
		        	"onTransitionEnd" 		: function() {
		        		$("#scroll-down-btn").removeClass("bounce animated");
		        		if($("#hp-slideshow .rs-gallery-caption p").length) {
		        			if($("#hp-slideshow .rs-caption").hasClass("hidden active")) {
			        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-title");
			        		} else {
			        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-caption");
			        		}
		        		} else {
		        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-title");
		        		}
		        	 },
		        	"watchVidClick" 		: function() { },
		        	"closeVidClick" 		: function() { },
		        	"allLoaded" 			: function() {
                    	$(".rs-gallery-images").after($(".rs-gallery-control.back"));
		        		$("#hp-slideshow  .rs-gallery-title").after("<div class='ui-clear'></div>");
		        		
		        		if($("#hp-slideshow .rs-gallery-caption p").length) {
		        			if($("#hp-slideshow .rs-caption").hasClass("hidden active")) {
			        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-title");
			        		} else {
			        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-caption");
			        		}
		        		} else {
		        			$("#hp-slideshow  .rs-gallery-desc-more-link").appendTo("#hp-slideshow  .rs-gallery-title");
		        		}
		        	 }
			});
		}
	}
	
	function contentRotator(){
		if(!$("#content-left-top #sw-content-container3.ui-hp .rs-gallery-images").length){
        	if($("#content-left-top #sw-content-container3.ui-hp .ui-widget.app.cs-rs-multimedia-rotator").length) {
				$("#sw-content-container3.ui-hp div.ui-widget.app.cs-rs-multimedia-rotator").csRsMultimediaRotator({
					"imageWidth" 			: 436,
		        	"imageHeight" 			: 244,
		        	"appHeader" 			: "yes",
		        	"titles" 			: "yes",
		        	"captions" 			: "yes",
		        	"galleryOverlay" 		: "no",
		       		"linkedElement" 		: ["image"], // ["image", "title", "overlay", "all"]
		        	"playPause" 			: "no",
		        	"mainControls" 			: "no",
		        	"contMoreLink" 			: "no",
		        	"contMoreLinkText" 		: "MORE",
		        	"descMoreLink" 			: "yes",
		        	"descMoreLinkText" 		: "PPS Video News Gallery",
		        	"bullets" 			: "no",
		        	"bulletControls" 		: "no",
		        	"thumbnails" 			: "yes",
		        	"thumbnailWidth" 		: 102,
		        	"thumbnailHeight" 		: 59,
		        	"thumbnailSlideDirection" 	: "horizontal", // horizontal or vertical
		        	"hoverPause" 			: "yes",
		        	"fullScreenRotator" 		: "no", // image dimensions work well at 1500x915
		        	"fullScreenVideo" 		: "yes",
		        	"transitionType" 		: "fade", // fade or slide
		        	"transitionSpeed" 		: 1,
		        	"transitionDelay" 		: 5,
					"autoRotate" 			: "no",
		        	"onImageLoad" 			: function() { },
		        	"allImagesLoaded" 		: function() { },
		        	"onThumbnailLoad" 		: function() { },
		        	"allThumbnailsLoaded" 		: function() { },
		        	"onTransitionStart" 		: function() { },
		        	"onTransitionEnd" 		: function() { },
		        	"watchVidClick" 		: function() { },
		        	"closeVidClick" 		: function() { },
		        	"allLoaded" 			: function() {
		        		$("<div class='video-title-container'><h1 class='portland-video-title'>Video</h1></div>").prependTo("#content-left-top #sw-content-container3.ui-hp div.ui-widget.app.cs-rs-multimedia-rotator .rs-gallery-desc");
		        	 	var videoNewsLink = "<a href='https://www.youtube.com/user/ppscomms' class='more-link'>View All Video News</a>";
		        	 	$(videoNewsLink).appendTo(".video-title-container");
		        	 	$("#content-left-top div.ui-widget.app.cs-rs-multimedia-rotator .rs-gallery-thumb-control").remove();
		        	 }
			});
		}
        }
	}
	
	function appView(){
		if(!appViewSet) {
			// DO NOT ADD APP INTO ACCORDION IF IT DOES NOT HAVE A HEADER
			$("#hp-content div.ui-widget.app").each(function() {
				if($(".ui-widget-header", this).hasClass("ui-helper-hidden") || jQuery.trim($(".ui-widget-header h1", this).text()) == "") {
					$(this).addClass("no-header");
				}
			});
			
			// ACCORDION HEADER CLICK
			$("#hp-content div.ui-widget-header").click(function(){
				if($(this).parent().hasClass("active")) {
					$(this).parent().removeClass("active");
				} else {
					var clickedApp = $(this).parent().parent().attr("id");
					$(this).parent().addClass("active");
				}
			});
		
			appViewSet = 1;
		}
	}
	
	function appViewReset() {
		if(appViewSet) {
			$("#hp-content div.ui-widget-header").unbind("click");
			
			appViewSet = 0;
		}
	}	
	
	function useChannelForSection(){
		//DOC use channel name for pagelistnavigation header if one is not present
		if(!$("div.sp.column.one .ui-widget-header h1").length > 0) {
			$("div.sp.column.one .ui-widget-header").append("<h1></h1>");
		}
        }
        
	function dynamicContent(){
		// ********** MYSTART ********** //
        $(".sw-mystart-button.home").remove();
		languageDropdown();
		//addFramesetTranslate();
		$("#gb-links-outer").before($("#sw-mystart-outer"));
		//DOC replace mystart dropdown arrows
		$(".sw-myaccount-chevron").text("");
		$(".sw-dropdown-arrow").text("");
		$(".sw-mystart-dropdown.translate .sw-dropdown-arrow").text("╲╱");
		//DOC change school dropdown text
		$(".sw-mystart-dropdown.schoollist .selector").text("Our Schools");
		//DOC change search button
		//$("#sw-search-button span").addClass("icon-font").text("s");
		
		//DOC fixes search box not allowing spaces
        $("#sw-search-input").before('<label for="sw-search-input" class="acc-hidden">Search Input</label>');
		$("#sw-mystart-search").unbind();
		$(".gb-icon").each(function(){
			if($(this).text() != "") {
				$(this).show();
			}
		});
		
		// ********** SLIDESHOW ********** //
		//SCROLL DOWN BTN
		$("#scroll-down-btn").click(function(){
			$("html, body").animate({
				scrollTop: $("#hp-content-outer").offset().top - 95
			}, 800, function(){
				$("html, body").stop(true, false);
			});
		});
		
		// ********** CHANNEL BAR ********** //
		//DOC add first class to first channel item
		$("li.sw-channel-item").first().addClass("first");
		//DOC add last class to last channel item
		$("li.sw-channel-item").last().addClass("last");
		$(".sw-channel-item").attr("aria-haspopup","true");
		$(".sw-channel-dropdown li").attr("aria-haspopup","false");
		//STAFF CHANNEL
		$(".sw-channel-item > a > span").each(function() {
			if($(this).text() == "Staff") {
				$(this).parent().parent().addClass("staff-channel");
			}
		});
        
        if($("#hp-alert-outer").height() == 0) {
        	$("#hp-alert-outer").remove();
        }
		
		// ********** BODY ********** //
		//DOC rename first breadcrumb
		$("li.ui-breadcrumb-first > a > span").text("Home");
		//DOC replace more-link text
		$("a.more-link span").text("View All News Articles");
		//Added by Marie - Case #00174090 and #00173751
		$(".ui-widget.app.staffdirectorydiv a.more-link span, .ui-widget.app.school-directory a.more-link span").text("View All");
		//DOC replace ui-return text for headlines
		$(".headlines.detail .ui-return span").text("« Return to Headlines");
		//DOC auto-focus username signin field
		$("#swsignin-txt-username").focus();
		//DOC center signed out message and signin button
        	if($("div.ui-spn > div > p > span").text() == "You have been signed out."){
        		$("div.ui-spn > div").css({"text-align" : "center", "padding" : "50px 0px 50px 0px"});
        		//DOC add signed out breadcrumb
        		$(".ui-breadcrumbs").append("<li>Signed Out</li>");
        	}
        	//DOC add sign in breadcrumb
        	if($("#swlogin").length){
        		$(".ui-breadcrumbs").append("<li>Sign In</li>");
        	}
        	//CHANGE LOGIN PAGE TEXT
        	$("#swlogin .ui-widget-header h1").text("PPS staff log in."); //changed text by Lyndsay - Case 00507556 - 4.18.17
        	
        	//MOD EVENTS
        	$(".upcomingevents").modEvents({
			columns:"yes",
			monthLong:"yes",
			dayWeek:"yes"
		});
		eventsByDay(".upcomingevents .ui-articles");
		if($("div.ui-widget.app.upcomingevents .ui-article-description").text() == "There are no upcoming events to display.") {
			$("div.ui-widget.app.upcomingevents .ui-article-description").css("padding","0px");
		}
		//MOVE EVENT MONTH AND DAY OF WEEK
		$(".upcomingevents .ui-article-title.sw-calendar-block-date").each(function() {
			$(this).after("<div class='cs-move-date'></div>");
			var csMonth = $(".joel-month",this);
			var csDay = $(".joel-day",this);
			$(".upcomingevents .cs-move-date").each(function() {
				$(csMonth).prependTo(this);
				$(csDay).appendTo(this);
			});
		});
		$(".upcoming-column.left").addClass("ui-clear");
		$("div.ui-widget.app.upcomingevents a.view-calendar-link span").text("View Full Calendar");
		$("div.ui-widget.app.upcomingevents").each(function() {
			$(this).find("a.view-calendar-link").appendTo($(this).find(".ui-widget-header h1"));
		});
		$("div.ui-widget.app.headlines").each(function() {
			$(this).find("a.more-link").appendTo($(this).find(".ui-widget-header h1"));
		});
		
		//DOC add read full story button to each healine
        	$(".ui-widget.app.headlines").each(function(){
        		$(".ui-article", this).each(function(){
        			var headlineURL = $("h1 a", this).attr("href");
        			$(".ui-article-description",this).append('<a class="custom-teaser-read-more" href="' + headlineURL + '">More</a>');
        		});
        	});
		//Added by Chris Case 00196442
		$('.ui-article-read-more a').text("More");
        	
        	//ADD UI-CLEAR CLASS TO UI-ARTICLES IN CONTENT CONTAINER 4 ON HP
        	$("#content-left-top #sw-content-container4 .ui-articles").addClass("ui-clear");
        	//REMOVE BORDER ON HEADLINES ARTICLE
        	$("#content-left-top #sw-content-container4 .ui-articles li:eq(1)").css("border","0 none");
        	
        	//CHECK IF QUICKLINKS IS USING AN IMAGE
        	$("#content-right-middle div.ui-widget.app.navigation li a").each(function() {
        		var quicklink = $(this).parent();
        		if($("img",this).length) {
        			$(quicklink).css("padding-left","0");
        		}
        	});
        	
        	//DOC add alt text to each quick link - ADDED BY MATT 092915
        	$("#content-right-middle .ui-widget.app.siteshortcuts").each(function(){
        		$(".ui-widget-detail .site-shortcuts li", this).each(function(){
        			var imageAlt = $("a img", this).attr("alt");
        			if(imageAlt) {
        				$("a", this).append('<span>' + imageAlt + '</span>');
                        $("a img", this).attr("alt","");
        			}
        		});
        	});
        	
        	//APPLY RESPONSIVE DIMENSIONS TO CONTENT IMAGES
        	$("div.ui-widget.app .ui-widget-detail img")
        		.not($("div.ui-widget.app.cs-rs-multimedia-rotator .ui-widget-detail img"))
        		.not($("div.ui-widget.app.gallery.json .ui-widget-detail img"))
                .not($("div.ui-widget.app.portland-headlines .ui-widget-detail img"))
        		.each(function() {
        			if ($(this).attr('width') !== undefined && $(this).attr('height') !== undefined) { // IMAGE HAS INLINE DIMENSIONS
					$(this).css({"display": "inline-block", "width": "100%", "max-width": $(this).attr("width") + "px", "height": "auto", "max-height": $(this).attr("height") + "px"});
				}
		});
		// CHANGE FORGOT PASSWORD HREF - ADDED BY BRENTON KELLY 11.3.2015
		//$("#swlogin a[title='I forgot my password.']").attr("href", "/forgot-password").attr("target", "_blank");
			$("#swlogin .ui-widget-footer .ui-widget-toolbar-bottom a.ui-btn-general.bb-butt").attr("href", "https://password.pps.net"); //Added by Sarah case 507556
        /* Added by Paul Nauman - 4.26.2016 - Custom Hourly for custom 404 page */
        if($(".ui-widget.error-404").length) {
        	$(".ui-widget div.ui-widget-detail.ui-two-col.col2").append("<div class='cs-custom-error-page'><div class='cs-custom-error-page-messages'><p>We're sorry, the page you requested can't be found.</p><p>很抱歉，无法找到您请求的页面。</p><p>К сожалению запрашиваемая Вами страница не может быть найдена</p><p>Waa ka xun nahay, bogga aad codsatay lama heli karo.</p><p>Lo sentimos, la página solicitada no se encuentra</p><p>Chúng tối xin cáo lối, khống có trang mạng mà quí vị muốn truy cập</p></div><div class='cs-custom-error-page-info'><p>If you typed the page address, please double-check that it is spelled correctly.</p></div><div class='cs-custom-error-page-info'><p>Otherwise, the page you're looking for might have been removed or had its name changed. Please <a onclick=\"AddAnalyticsEvent('Error', '404', 'Contact Link'); LoadContactInfo()\">email the PPS Webteam</a> to report this error, or use the links or search box above to find the correct page.</p></div></div>");
        }
        
        
        $("#cs-language-inner a").focus(function() {
        	$(".sw-mystart-dropdown.schoollist .sw-dropdown").attr("aria-hidden","true").hide();
        });
        $("#sw-search-input").focus(function() {
        	$("#sw-myaccount-list").attr("aria-hidden","true").hide();
        });
        
		// ********** FOOTER ********** //
		//DOC get Schoolwires footer links
		var feedbackURL = $("div#sw-footer-links li:eq(0) a").attr("href");
		var termsURL = $("div#sw-footer-links li:eq(2) a").attr("href");
		var policyURL = $("div#sw-footer-links li:eq(1) a").attr("href");
		
		$("a#feedback").attr("href", feedbackURL);
		$("a#terms").attr("href", termsURL);
		$("a#policy").attr("href", policyURL);			
	}
	
	function eventsByDay(container) {
		var countLi = 0;
		$(".ui-article", container).each(function(){
			if (!$(this).find("h1.ui-article-title.sw-calendar-block-date").size()){
				var moveArticle = $(this).html();
				$(this).parent().prev().children().children().next().append(moveArticle);
				$(this).parent().remove();
			};
		});
	}
	
	function searchText() {
		//DOC custom mystart search placeholder
	        //SEARCH TEXT
	        
	        //Added by Chris Case 00198397
	        $('div#sw-mystart-search input#sw-search-input').attr('placeholder', 'Search');
	        
	        //$("div#sw-mystart-search input#sw-search-input").removeAttr("placeholder");
		//$("div#sw-mystart-search input#sw-search-input").attr("value", "Search");
		/*$("div#sw-mystart-search input#sw-search-input").focus(function() {
			if($(this).val() == "Search") {
				$(this).val("");
			}
		});
		$("div#sw-mystart-search input#sw-search-input").blur(function() {
			if($(this).val() == "") {
				$(this).val("Search");
			}
		});*/
	}
	
	function calendarListView(){
		//DOC auto click function for schoolwires calendar list view for small mobile devices
		if(!$("a.ui-btn-toggle.list").hasClass("active")){
			$("a.ui-btn-toggle.list").click();
		}
	}
	
	function imgResize(){
		var imgHeight = $("#hp-slideshow .rs-gallery-images > img").height();
		//$("#hp-slideshow").height(imgHeight);
	}
	
	function addFramesetTranslate(){
		var pageURL = window.location.href;
		pageURL = encodeURIComponent(pageURL);
		var translateDropdown = '<div class="sw-mystart-dropdown translate">' +
							'<div class="selector">Google Translate</div>' +
							'<div class="sw-dropdown" style="display: none;">' +
								'<div class="sw-dropdown-selected">Google Translate</div>' +
								'<ul class="sw-dropdown-list">' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|es&hl=en&anno=0&ie=UTF8">English to Spanish</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|ar&hl=en&anno=0&ie=UTF8">English to Arabic</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|zh-CN&hl=en&anno=0&ie=UTF8">English to Chinese (Simplified)</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|zh-TW&hl=en&anno=0&ie=UTF8">English to Chinese (Traditional)</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|fr&hl=en&anno=0&ie=UTF8">English to French</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|de&hl=en&anno=0&ie=UTF8">English to German</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|it&hl=en&anno=0&ie=UTF8">English to Italian</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|ja&hl=en&anno=0&ie=UTF8">English to Japanese</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|ko&hl=en&anno=0&ie=UTF8">English to Korean</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|pt&hl=en&anno=0&ie=UTF8">English to Portuguese</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|ru&hl=en&anno=0&ie=UTF8">English to Russian</a>' +
									'</li>' +
								'</ul>' +
							'</div>' +
							'<div class="sw-dropdown-arrow"></div>' +
						'</div>';
		var pageHost = window.location.host;
		if(pageHost != "translate.googleusercontent.com")
		{				
			$("#gb-footer-translate").append(translateDropdown);
			$(".sw-mystart-dropdown.translate").click(function()
			{
				$(".sw-dropdown",this).show();
			});
			$(".sw-mystart-dropdown.translate").hover(function()
			{
			},
			function()
			{
				$(".sw-dropdown",this).hide();
			});
		}
		
	}

	
</script>

