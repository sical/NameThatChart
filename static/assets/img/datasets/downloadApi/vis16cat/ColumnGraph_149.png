<script type="text/javascript">

var domain = 'https://adobe--c.na20.visual.force.com';         // prod

var inSfdc = false;
var message = 'test-message';
 
window.addEventListener('message',function(event) {
    if(event.origin !== domain) {
        return;
    }
    inSfdc = true;
},false);
 
function clickJackWrapUp() {
  if (inSfdc == false) {
      top.location = self.location;
  }
  else {
    var d = document.getElementById('antiClickjack'); 
    d.parentNode.removeChild(d);
    document.body.className+=' hide-feedback';
  }
}
 
if (self === top) {
  var d = document.getElementById('antiClickjack'); 
  d.parentNode.removeChild(d);
    
} else {
  parent.postMessage(message,domain); //send the message and target URI
  setTimeout(clickJackWrapUp,100);
}
</script>


