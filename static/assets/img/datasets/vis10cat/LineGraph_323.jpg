		<script>
		jQuery(function(j){
			// no sub-nav on touch
			if(j('body').hasClass('touch'))
				return;

			var container = j('#header-790_container');
			var popup;

			function hideLinksPopup() {
				if(popup) {
					popup.glowPopUpPanel('hide');
				}
			}

			function showLinksPopUp(parent, children) {
				var links = j('<ul></ul>').append(children);
				links.on('mouseleave', function(){
					hideLinksPopup();
				});

				if(popup) {
					hideLinksPopup();
					popup.glowPopUpPanel('empty', '')
						.glowPopUpPanel('append', links)
						.glowPopUpPanel('show', parent);
				} else {
					popup = j('<div></div>')
						.glowPopUpPanel({
							cssClass: 'links-popup-panel',
							zIndex: 1500,
							hideOnDocumentClick: true
						})
						.glowPopUpPanel('append', links)
						.glowPopUpPanel('show', parent)
				}
			}

			container.on('mouseover', '.navigation-list-item a', function(e){
				var elm = j(e.target);
				var children = elm.data('_link_children');
				if(!children) {
					children = elm.closest('li').find('ul.children > li');
					elm.data('_link_children', children);
				}
				if(children && children.length > 0) {
					showLinksPopUp(elm, children);
				}
			});

			container.on('mouseout', '.navigation-list-item a', function(e){
				if(!(e.relatedTarget &&
					(j(e.relatedTarget).is('.links-popup-panel') ||
					j(e.relatedTarget).closest('.links-popup-panel').length > 0)))
					hideLinksPopup();
			});
		});
		</script>
