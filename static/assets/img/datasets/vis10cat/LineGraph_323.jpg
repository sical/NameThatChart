            j('#fragment-39833').on('click', '.answer-status a', function(e){
                e.preventDefault();
                var link = j(this),
                    threadItem = link.closest('.content-item');
                    previewItem = threadItem.next('.preview');
                    container = previewItem.find('.content');

                if(!container.is(':empty')) {
                    container.empty();
                    threadItem.removeClass('previewing');
                    previewItem.removeClass('previewing');
                } else {
                    threadItem.addClass('previewing');
                    previewItem.addClass('previewing');
                    j.telligent.evolution.get({
                        url: '/utility/scripted-file.ashx?_cf=answer.vm\u0026_fid=39833\u0026_ct=page\u0026_cp=common-home\u0026_ctt=0c647246673542f9875dc8b991fe739b\u0026_ctc=00000000000000000000000000000000\u0026_ctn=3fc3f82483d14ec485ef92e206116d49\u0026_cc=0\u0026_pcurl=~%2F',
