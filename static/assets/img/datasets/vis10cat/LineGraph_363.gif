<script>
function GetStockResults() {
    $.ajax(
        {
            type: "GET",
            url: '/StockQuote.php',
            async: true,
            cache: false,
            contentType: "application/xml; charset=utf-8"
        })
        .done(function (data) {
            var xml = $(data);
            var triangle = '&nbsp;<div id="stockQouteTrend" class="investor-stock-trend"><div class="investor-stock-triangle"></div><div class="investor-stock-tail"></div></div>';
            $("#stockQoutePrice").html(xml.find("Currency").text() + ' ' + xml.find("CurrentPrice").text()+ triangle);
            $("#stockQouteSymbol").append(xml.find("Symbol").text());
            $("#stockQouteDate").append("date:" + xml.find("Date").text() + " " + xml.find("Time").text());

            var change = xml.find("Change").text();
            if (change.charAt(0) == "-") {
                $("#stockQouteTrend").addClass("investor-stock-down");
            }
            else {
                $("#stockQouteTrend").addClass("investor-stock-up");
            }
        });
};
$(document).ready(function () {
    GetStockResults();
});
</script>

