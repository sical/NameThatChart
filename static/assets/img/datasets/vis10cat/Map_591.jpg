<script type="text/javascript">
  var nolardUrl = '';
  if (nolardUrl != '') {
  if ($('#toc-download-book-pdf-link').length || $('#download-book-epub-link').length) {
  reportParticipation(nolardUrl, 'core-bookpage-book-download', 'baseline');
  if ($('#toc-pdf-link').length) {
  reportParticipation(nolardUrl, 'core-bookpage-chapter-download', 'baseline');
  }
  reportForMouseClick('#toc-download-book-pdf-link', nolardUrl, 'core-bookpage-book-download', 'baseline');
  reportForMouseClick('#action-bar-download-book-pdf-link', nolardUrl, 'core-bookpage-book-download', 'baseline');
  reportForMouseClick('#download-book-epub-link', nolardUrl, 'core-bookpage-book-download', 'baseline');
  reportForMouseClick('#toc-pdf-link', nolardUrl, 'core-bookpage-chapter-download', 'baseline');
  }
  }
  
  function reportParticipation(nolardUrl, experiment, abgroup) {
  $.ajax({ url: nolardUrl + '/participate/' + experiment + '/' + abgroup, method:"POST" });
  }
  
  function reportConversion(nolardUrl, experiment, abgroup) {
  $.ajax({ url: nolardUrl + '/convert/' + experiment + '/' + abgroup, method:"POST" });
  }
  
  function reportForMouseClick(linkCssSelector, nolardUrl, experiment, abgroup) {
  var counter = 0;
  $('body').delegate(linkCssSelector, 'click', function() {
  if(counter == 0) {
  reportConversion(nolardUrl, experiment, abgroup);
  counter++;
  }
  });
  }
</script>

