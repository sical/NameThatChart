<script type="text/javascript">
    function clear_search(o) {
        if (o.value == 'search') { o.value = ''; }
    }

    function set_search(o) {
        if (o.value == '') { o.value = 'search'; }
    }

    // Function retrieved by Steven from: http://www.webtoolkit.info/javascript-url-decode-encode.html
    function URLEncode(string) 
    {
		string = string.replace(/\r\n/g,"\n");
		var utftext = "";
 
		for (var n = 0; n < string.length; n++) {
			var c = string.charCodeAt(n);
			if (c < 128) {
				utftext += String.fromCharCode(c);
			}
			else if((c > 127) && (c < 2048)) {
				utftext += String.fromCharCode((c >> 6) | 192);
				utftext += String.fromCharCode((c & 63) | 128);
			}
			else {
				utftext += String.fromCharCode((c >> 12) | 224);
				utftext += String.fromCharCode(((c >> 6) & 63) | 128);
				utftext += String.fromCharCode((c & 63) | 128);
			}
		}
		return escape(utftext);
	}

    // Retrieved from: http://www.htmlcodetutorial.com/forms/index_famsupp_157.html
    function SubmitOnEnter(field, e)
    {
	    var keycode;
	    if (window.event)
		    keycode = window.event.keyCode;
	    else if (e)
		    keycode = e.which;
	    else
		    return true;
	    if (keycode == 13)
	    {
	        document.getElementById('ctl00_masterTopContainer_Block_673_btnSearch').click();
		    return false;
	    }
	    else
		    return true;
    }

    function SearchSite()
    {
        var keyword = document.getElementById('ctl00_masterTopContainer_Block_673_txtSearch').value;
        if (/^\s*$/.test(keyword)) {
            document.getElementById('ctl00_masterTopContainer_Block_673_txtSearch').focus();
            return;
        }
        window.open('http://www.google.com/cse?cx=012149137107551921584%3Anypef31stuq&ie=UTF-8&q=' + URLEncode(keyword) + '&sa.x=40&sa.y=12'); 
    }
</script>
